<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <!--从application.yml导入属性配置-->
<!--    <properties resource="application.properties"/>-->

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!--配置分页插件-->
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"/>

        <!--配置jdbc连接信息-->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://39.102.133.112:3306/AI_INFO?useUnicode=true;characterEncoding=utf8;serverTimezone=UTC"
                        userId="root"
                        password="2413">
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>

        <!--类型转换-->
        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!--生成实体类地址-->
        <javaModelGenerator targetPackage="com.ljd.ai.model" targetProject="src\main\java">
            <property name="enableSubPackages" value="false" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!-- 生成mapper.xml文件 -->
        <sqlMapGenerator targetPackage="mapper"  targetProject="src\main\resources">
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>

        <!-- 生成mapxml对应client，也就是接口dao -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.ljd.ai.mapper"  targetProject="src\main\java">
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>

        <!--配置数据库表-->
<!--        <table tableName="t_activity" domainObjectName="Activity" />-->
<!--        <table tableName="t_admin" domainObjectName="Admin" />-->
<!--        <table tableName="t_browse" domainObjectName="Browse" />-->
<!--        <table tableName="t_exchange" domainObjectName="Exchange" />-->
<!--        <table tableName="t_goods" domainObjectName="Goods" />-->
<!--        <table tableName="t_praise" domainObjectName="Praise" />-->
        <table tableName="IMAGE_INFO" domainObjectName="ImageInfo" />
<!--        <table tableName="t_type" domainObjectName="Type" />-->
<!--        <table tableName="t_user" domainObjectName="User" />-->


    </context>
</generatorConfiguration>